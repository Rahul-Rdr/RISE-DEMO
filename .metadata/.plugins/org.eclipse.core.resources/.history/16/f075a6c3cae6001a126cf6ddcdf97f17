public class AutomateAC {

	public static void main(String[] args) {
		

		LocalTime endTime = LocalTime.parse( "17:29" ) ;
		
		while(!endTime.equals(java.time.LocalTime.now())) {
			
			System.out.println("its working");
		}
		  
	}

}
